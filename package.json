{
  "name": "emmanuelgautier.fr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "next build",
    "build": "run-s extract:i18n compile:i18n build:rss build:next",
    "build:css": "CSS_ENV=build postcss styles/globals.css --config postcss.config.js -o styles/output.css",
    "build:next": "next build",
    "build:rss": "node ./scripts/generate-rss.js",
    "export": "next export",
    "start": "next start",
    "serve": "serve out",
    "extract:i18n": "formatjs extract '{pages,components}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang",
    "postbuild": "next-sitemap",
    "check:format": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn check:format",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@formatjs/cli": "2.15.0",
    "@formatjs/intl-datetimeformat": "2.8.4",
    "@formatjs/intl-getcanonicallocales": "1.5.3",
    "@formatjs/intl-numberformat": "5.7.6",
    "@formatjs/intl-pluralrules": "3.5.6",
    "@formatjs/intl-relativetimeformat": "7.3.6",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@mapbox/rehype-prism": "0.6.0",
    "@netlify/plugin-lighthouse": "2.1.0",
    "@tailwindcss/typography": "0.4.0",
    "autoprefixer": "10.2.1",
    "babel-plugin-react-intl": "8.2.25",
    "classnames": "2.3.1",
    "clsx": "1.1.1",
    "date-fns": "2.21.1",
    "gray-matter": "4.0.2",
    "lodash": "4.17.20",
    "netlify-purge-cloudflare-on-deploy": "1.2.0",
    "next": "10.2.0",
    "next-optimized-images": "2.6.2",
    "next-seo": "4.24.0",
    "next-sitemap": "1.6.57",
    "next-themes": "0.0.14",
    "postcss": "8.2.15",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-intl": "5.17.1",
    "react-schemaorg": "1.3.1",
    "react-share": "4.4.0",
    "remark": "13.0.0",
    "remark-external-links": "8.0.0",
    "remark-html": "13.0.1",
    "remark-parse": "9.0.0",
    "remark-prism": "1.3.4",
    "remark-rehype": "8.1.0",
    "remark-stringify": "9.0.1",
    "rss": "1.2.2",
    "schema-dts": "0.8.2",
    "tailwindcss": "2.1.2",
    "unified": "9.2.0"
  },
  "devDependencies": {
    "@emmanuelgautier/renovate-config": "1.2.0",
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@netlify/plugin-nextjs": "3.3.0",
    "@tailwindcss/jit": "0.1.18",
    "@types/classnames": "2.3.1",
    "@types/node": "14.17.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/rss": "0.0.28",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "cross-env": "7.0.3",
    "cssnano": "5.0.1",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "6.0.0",
    "image-trace-loader": "1.0.2",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "lint-staged": "11.0.0",
    "lqip-loader": "2.2.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "8.3.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.0",
    "responsive-loader": "2.3.0",
    "serve": "11.3.2",
    "sharp": "0.28.0",
    "svg-sprite-loader": "6.0.2",
    "typescript": "4.2.4",
    "webp-loader": "0.6.0"
  }
